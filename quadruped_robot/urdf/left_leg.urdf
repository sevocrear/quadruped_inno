<?xml version="1.0" encoding="utf-8"?>
<robot name="quadruped_robot" xmlns:xacro="http://ros.org/wiki/xacro">

    <!-- LEFT FORWARD MOTOR -->

    <xacro:macro name="left_motor" params="link_prefix joint_xyz joint_rpy">
      <link
    name="${link_prefix}_motor">
    <inertial>
      <origin
        xyz="-2.4016E-06 -0.00012687 -0.01825"
        rpy="0 0 0" />
      <mass
        value="1.7" />
      <inertia
        ixx="0.0011015"
        ixy="-2.2115E-20"
        ixz="-4.7092E-20"
        iyy="0.0011135"
        iyz="-7.75E-06"
        izz="0.0018907" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://quadruped_robot/meshes/left_forward_motor.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.098039 0.098039 0.098039 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://quadruped_robot/meshes/left_forward_motor.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="body_${link_prefix}_motor"
    type="fixed">
    <origin
      xyz="${joint_xyz}"
      rpy="${joint_rpy}" />
    <parent
      link="body" />
    <child
      link="${link_prefix}_motor" />
    <axis
      xyz="0 0 0" />
  </joint>
  </xacro:macro>

  <!-- LEFT LEG -->
  <xacro:macro name="left_leg" params="link_prefix joint_xyz joint_rpy">
  <link
    name="${link_prefix}_leg">
    <inertial>
      <origin
        xyz="-0.00291689067610592 -0.000123327968837258 -0.00111336451391641"
        rpy="0 0 0" />
      <mass
        value="1.15" />
      <inertia
        ixx="0.00194402199468823"
        ixy="-7.74995368724056E-06"
        ixz="2.20326276361798E-11"
        iyy="0.00113022776843808"
        iyz="5.94322688269652E-11"
        izz="0.00115440199269891" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://quadruped_robot/meshes/left_forward_leg.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="1 0.937254901960784 0.137254901960784 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://quadruped_robot/meshes/left_forward_leg.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="${link_prefix}_motor_leg"
    type="revolute">
    <origin
      xyz="${joint_xyz}"
      rpy="${joint_rpy}" />
    <parent
      link="${link_prefix}_motor" />
    <child
      link="${link_prefix}_leg" />
    <axis
      xyz="0 0 -1" />
    <limit
      lower="-0.78"
      upper="0.78"
      effort="50"
      velocity="30" />
    <!-- <dynamics damping = "0.5" friction = "0.1"/> -->
  </joint>
  </xacro:macro>


  <!--  LEFT HIP -->
  <xacro:macro name="left_hip" params="link_prefix joint_xyz joint_rpy">
  <link
    name="${link_prefix}_hip">
    <inertial>
      <origin
        xyz="-0.000122004696831546 -0.0105068917015962 0.0369232159258762"
        rpy="0 0 0" />
      <mass
        value="0.3" />
      <inertia
        ixx="0.0025944"
        ixy="2.612E-05"
        ixz="4.1998E-06"
        iyy="0.0012986"
        iyz="1.2724E-05"
        izz="0.0035306" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://quadruped_robot/meshes/left_forward_hip.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="1 0.937254901960784 0.137254901960784 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://quadruped_robot/meshes/left_forward_hip.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="${link_prefix}_leg_hip"
    type="revolute">
    <origin
      xyz="${joint_xyz}"
      rpy="${joint_rpy}"/>
    <parent
      link="${link_prefix}_leg" />
    <child
      link="${link_prefix}_hip" />
    <axis
      xyz="0 0 -1" />
    <limit
      lower="-1.31"
      upper="1.31"
      effort="50"
      velocity="30" />
    <!-- <dynamics damping = "0.5" friction = "0.1"/> -->
  </joint>
  </xacro:macro>


  <!-- LEFT CALF -->
  <xacro:macro name="left_calf" params="link_prefix joint_xyz joint_rpy">
  <link
    name="${link_prefix}_calf">
    <inertial>
      <origin
        xyz="1.33042839911279E-05 0.0735129471212146 1.01143462072351E-05"
        rpy="0 0 0" />
      <mass
        value="0.1" />
      <inertia
        ixx="0.0"
        ixy="0.0"
        ixz="0.0"
        iyy="0.0"
        iyz="0.0"
        izz="0.0" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://quadruped_robot/meshes/left_forward_calf.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.101960784313725 0.101960784313725 0.101960784313725 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://quadruped_robot/meshes/left_forward_calf.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="${link_prefix}_hip_calf"
    type="revolute">
    <origin
      xyz="${joint_xyz}"
      rpy="${joint_rpy}"/>
    <parent
      link="${link_prefix}_hip" />
    <child
      link="${link_prefix}_calf" />
    <axis
      xyz="0 0 -1" />
    <limit
      lower="-2.6"
      upper="2.6"
      effort="50"
      velocity="30" />
    <!-- <dynamics damping = "0.5" friction = "0.1"/> -->
  </joint>
  <link
    name="${link_prefix}_foot">
    <inertial>
      <origin
        xyz="2.69840204902838E-07 -0.0143807852672446 -1.27049741839858E-05"
        rpy="0 0 0" />
      <mass
        value="0.04" />
      <!-- <mass
        value="50" /> -->
      <inertia
        ixx="1.41888786495424E-05"
        ixy="-2.90568059491003E-10"
        ixz="-4.06238332327928E-11"
        iyy="8.65121074865133E-06"
        iyz="3.97669015260362E-09"
        izz="1.41178031393142E-05" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://quadruped_robot/meshes/left_forward_foot.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.298039215686275 0.298039215686275 0.298039215686275 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://quadruped_robot/meshes/left_forward_foot.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="${link_prefix}_calf_foot"
    type="fixed">
    <origin
      xyz="0 0.1775 0"
      rpy="0 0 3.1416" />
    <parent
      link="${link_prefix}_calf" />
    <child
      link="${link_prefix}_foot" />
    <axis
      xyz="0 0 0" />
  </joint>
  <dynamics friction = "1000"/>
  </xacro:macro>

</robot>