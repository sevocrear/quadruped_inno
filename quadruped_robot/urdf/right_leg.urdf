    <limit
      lower="-1.57"
      upper="1.57"
      effort="24"
      velocity="50" />
  </joint>
  <link
    name="${link_prefix}_foot">
    <inertial>
      <origin
        xyz="2.67734406894649E-07 -0.0143807830353894 -1.27018434094295E-05"
        rpy="0 0 0" />
      <mass
        value="0.0424093690699508" />
      <inertia
        ixx="1.41888803190933E-05"
        ixy="-2.89152058115048E-10"
        ixz="-3.96352551681418E-11"
        iyy="8.6512128643686E-06"
        iyz="3.97404573019817E-09"
        izz="1.41178047876361E-05" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://quadruped_robot/meshes/left_back_foot.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.298039215686275 0.298039215686275 0.298039215686275 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://quadruped_robot/meshes/left_back_foot.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="${link_prefix}_calf_foot"
    type="fixed">
    <origin
      xyz="0.003139 0.17761 0.013"
      rpy="0 0 3.1416" />
    <parent
      link="${link_prefix}_calf" />
    <child
      link="${link_prefix}_foot" />
    <axis
      xyz="0 0 0" />
  </joint>
  </xacro:macro>

</robot>